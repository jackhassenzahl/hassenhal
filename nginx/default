server {
    listen 80;
    server_name assassin.hassenhal.com;

    root /var/www/html/hassenhal;
	index assassin/index.html;

    location / {
		try_files $uri $uri/ =404;
	}
}

server {
    listen 80;
    server_name gti.hassenhal.com;

    root /var/www/html/hassenhal;
	index gti/index.html;

    location / {
		try_files $uri $uri/ =404;
	}
}

server {
    listen 80;
    server_name hassenhal.com www.hassenhal.com;

    root /var/www/html/hassenhal;
	index index.html;

	location / {
		try_files $uri $uri/ =404;
	}
}
